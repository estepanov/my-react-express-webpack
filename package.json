{
  "name": "my-react-express-webpack",
  "version": "1.0.0",
  "description": "My Custom built boilerplate with the latest versions of React, Redux, Webpack, Babel, Node, Express",
  "main": "dist/index.js",
  "repository": "git@github.com:estepanov/my-react-express-webpack.git",
  "author": "Evans A. Stepanov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "babel-loader": "^8.0.0-beta",
    "concurrently": "^3.6.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "start": "concurrently \"npm run dev-server\" \"npm run build-client-watch\"",
    "build-client-watch": "webpack-dev-server -w --inline --open --hot",
    "dev-server": "nodemon src/server/index.js",
    "production-server": "node src/server/index.js",
    "lint": "eslint ./ --ignore-path .gitignore"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6"
  }
}
